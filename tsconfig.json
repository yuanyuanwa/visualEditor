{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"lib": [
			"esnext",
			"dom",
			"dom.iterable",
			"scripthost"
		] ,
		"jsx": "preserve" ,
		"isolatedModules": true,
		"strict": true,
		"moduleResolution": "node" ,
		"baseUrl": "./" ,
		"paths": {
			"/@/*": [
				"src/*",
				"./src/*"
			]
		} ,
		"types": [
			"vite/client",
		] ,
		"allowSyntheticDefaultImports": true ,
		"esModuleInterop": true,
		"experimentalDecorators": true ,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true 
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.vue",
		"src/**/*.tsx",
		"src/**/*.d.ts"
, "src/components/tmp/index.js"	], 
	"exclude": [
		"node_modules",
		"dist"
	] 
}